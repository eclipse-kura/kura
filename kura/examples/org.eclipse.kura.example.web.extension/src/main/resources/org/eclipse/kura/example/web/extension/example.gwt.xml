<module rename-to='exampleext'>

  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User' />
  <inherits name="org.eclipse.kura.web2.ext" />
  <inherits name='org.gwtbootstrap3.GwtBootstrap3'/>

  <!-- Specify the app entry point class.                         -->
  <entry-point class='org.eclipse.kura.example.web.extension.client.ExampleEntryPoint' />

  <!-- Specify the paths for translatable code                    -->
  <source path='client' />
  <source path='shared' />
  <set-property name="user.agent" value="ie9,ie10,gecko1_8,safari"/>

</module>
