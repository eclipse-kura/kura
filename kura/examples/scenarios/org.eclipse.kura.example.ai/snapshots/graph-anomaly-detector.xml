var outRecord = newWireRecord()
outRecord['LED_MATRIX_CHARS'] = newStringValue("ANOMALY")

outRecord['LED_MATRIX_FRONT_COLOR_B'] = newFloatValue(0.0)
outRecord['LED_MATRIX_BACK_COLOR_B'] = newFloatValue(0.0)

if(inRecord.ANOMALY0.getValue() == true) {
  outRecord['LED_MATRIX_FRONT_COLOR_R'] = newFloatValue(1.0)
  outRecord['LED_MATRIX_BACK_COLOR_R'] = newFloatValue(1.0)
  outRecord['LED_MATRIX_FRONT_COLOR_G'] = newFloatValue(0.0)
  outRecord['LED_MATRIX_BACK_COLOR_G'] = newFloatValue(0.0)
} else {
  outRecord['LED_MATRIX_FRONT_COLOR_R'] = newFloatValue(0.0)
  outRecord['LED_MATRIX_BACK_COLOR_R'] = newFloatValue(0.0)
  outRecord['LED_MATRIX_FRONT_COLOR_G'] = newFloatValue(1.0)
  outRecord['LED_MATRIX_BACK_COLOR_G'] = newFloatValue(1.0)
}

output.add(outRecord)</esf:value>
