      <dropinLocation
          rootFolder="${git.clone.location}/target-platform/usb4java-javax/target"
          recursive="false"/>
      <dropinLocation
          rootFolder="${git.clone.location}/target-platform/org.eclipse.soda.dk.comm/target"
          recursive="false"/>
    </targlet>
    <description>Kura</description>
  </setupTask>
  <setupTask
      xsi:type="setup.workingsets:WorkingSetTask">
    <workingSet
        name="${scope.project.label}">
      <predicate
          xsi:type="predicates:AndPredicate">
        <operand
            xsi:type="predicates:NamePredicate"
            pattern="org.eclipse.kura.*"/>
        <operand
            xsi:type="predicates:NotPredicate">
          <operand
              xsi:type="predicates:NamePredicate"
              pattern="org.eclipse.kura.*\.test.*"/>
        </operand>
        <operand
            xsi:type="predicates:NotPredicate">
          <operand
              xsi:type="predicates:NamePredicate"
              pattern="org.eclipse.kura.example.*"/>
        </operand>
        <operand
            xsi:type="predicates:NotPredicate">
          <operand
              xsi:type="predicates:NamePredicate"
              pattern="org.eclipse.kura.demo.*"/>
        </operand>
      </predicate>
    </workingSet>
    <workingSet
        name="Karaf">
      <predicate
          xsi:type="predicates:OrPredicate">
        <operand
            xsi:type="predicates:LocationPredicate"
            pattern=".*\/karaf\/.*"/>
        <operand
            xsi:type="predicates:NamePredicate"
            pattern="karaf"/>
      </predicate>
    </workingSet>
    <workingSet
        name="Target Platform">
      <predicate
          xsi:type="predicates:OrPredicate">
        <operand
            xsi:type="predicates:LocationPredicate"
            pattern=".*/target-platform.*"/>
        <operand
            xsi:type="predicates:LocationPredicate"
            pattern=".*/target-definition.*"/>
      </predicate>
    </workingSet>
    <workingSet
        name="Features"
        id="">
      <predicate
          xsi:type="predicates:AndPredicate">
        <operand
            xsi:type="predicates:OrPredicate">
          <operand
              xsi:type="predicates:LocationPredicate"
              pattern=".*/features/.*"/>
          <operand
              xsi:type="predicates:NamePredicate"
              pattern=".*\.feature.*"/>
        </operand>
        <operand
            xsi:type="predicates:NotPredicate">
          <operand
              xsi:type="predicates:LocationPredicate"
              pattern=".*\/karaf\/.*"/>
        </operand>
      </predicate>
    </workingSet>
    <workingSet
        name="Tests">
      <predicate
          xsi:type="predicates:OrPredicate">
        <operand
            xsi:type="predicates:LocationPredicate"
            pattern=".*/test/.*"/>
        <operand
            xsi:type="predicates:NamePredicate"
            pattern="org.eclipse.kura.*\.test.*"/>
      </predicate>
    </workingSet>
    <workingSet
        name="Examples">
      <predicate
          xsi:type="predicates:OrPredicate">
        <operand
            xsi:type="predicates:LocationPredicate"
            pattern=".*/examples/.*"/>
        <operand
            xsi:type="predicates:NamePredicate"
            pattern="org.eclipse.kura.example.*"/>
        <operand
            xsi:type="predicates:NamePredicate"
            pattern="org.eclipse.kura.demo.*"/>
      </predicate>
    </workingSet>
    <description>The dynamic working sets for ${scope.project.label}</description>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      name="Developer Type">
    <setupTask
        xsi:type="setup:CompoundTask"
        filter="(kura.developer.type=user)"
        name="User">
      <setupTask
          xsi:type="maven:MavenImportTask">
        <sourceLocator
            rootFolder="${git.clone.location}/kura/org.eclipse.kura.api"/>
        <sourceLocator
            rootFolder="${git.clone.location}/kura/emulator/org.eclipse.kura.emulator"/>
        <sourceLocator
            rootFolder="${git.clone.location}/kura/examples/org.eclipse.kura.demo.heater"/>
      </setupTask>
    </setupTask>
    <setupTask
        xsi:type="setup:CompoundTask"
        filter="(kura.developer.type=developer)"
        name="Developer">
      <setupTask
          xsi:type="launching:LaunchTask"
          id="build-full"
          launcher="setups/launchers/build-full.launch">
        <description>Build Kura Core</description>
      </setupTask>
      <setupTask
          xsi:type="maven:MavenImportTask"
          id="import.core"
          predecessor="build-full"
          projectNameTemplate="">
        <sourceLocator
            rootFolder="${git.clone.location}/kura"
            locateNestedProjects="true">
          <predicate
              xsi:type="predicates:AndPredicate">
            <operand
                xsi:type="predicates:NotPredicate">
              <operand
                  xsi:type="predicates:LocationPredicate"
                  pattern=".*/org.eclipse.kura.windows.*"/>
            </operand>
          </predicate>
        </sourceLocator>
        <sourceLocator
            rootFolder="${git.clone.location}/karaf"/>
        <sourceLocator
            rootFolder="${git.clone.location}/kura/examples"
            locateNestedProjects="true"/>
        <sourceLocator
            rootFolder="${git.clone.location}/kura/features"
            locateNestedProjects="true"/>
      </setupTask>
    </setupTask>
  </setupTask>
  <stream name="develop"
      label="Development"/>
  <logicalProjectContainer
      xsi:type="setup:ProjectCatalog"
      href="index:/org.eclipse.setup#//@projectCatalogs[name='org.eclipse']"/>
  <description>Eclipse Kura provides cool stuff.</description>
</setup:Project>
