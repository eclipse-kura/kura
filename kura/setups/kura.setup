            rootFolder="${git.clone.location}/target-platform">
          <projectFactory
              xsi:type="resources:MavenProjectFactory"/>
          <predicate
              xsi:type="predicates:LocationPredicate"
              pattern=".*/target-platform$"/>
        </sourceLocator>
      </setupTask>
      <setupTask
          xsi:type="launching:LaunchTask"
          launcher="setups/launchers/build-target-platform.launch"/>
    </setupTask>
  </setupTask>
  <setupTask
      xsi:type="setup.targlets:TargletTask"
      filter="">
    <targlet
        name="${scope.project.label}"
        activeRepositoryList="${eclipse.target.platform}"
        includeAllPlatforms="true">
      <requirement
          name="*"/>
      <dropinLocation
          rootFolder="${git.clone.location}/target-platform/p2-repo-common/target/repository"/>
      <dropinLocation
          rootFolder="${git.clone.location}/target-platform/p2-repo-equinox_3.8.1/target/repository"/>
      <dropinLocation
          rootFolder="${git.clone.location}/target-platform/p2-repo-equinox_3.12.50/target/repository"/>
      <dropinLocation
